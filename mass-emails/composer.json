{
    "name": "koisystems/mass-emails",
    "description": "package created to send multiples emails with filters in laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jhony Pereira do Vale",
            "email": "jhony.vale@koisys.com",
            "homepage": "https://github.com/JPdesconectado/mass-emails"
        }
    ],
    "homepage": "https://github.com/JPdesconectado/mass-emails",
    "keywords": ["Laravel", "MassEmails"],
    "require": {
        "illuminate/support": "~7|~8",
        "livewire/livewire": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Koisystems\\MassEmails\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Koisystems\\MassEmails\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
